{
  "$id": "https://vda231-301.github.io/schemas/VDA_278/VDA_231-301_VDA_278_2016-05.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "VDA 231-301 testing project JSON schema for VDA 278 2016-05",
  "type": "object",
  "allOf": [
    {
      "$ref": "https://vda231-301.github.io/schemas/generic/VDA_231-301_generic_v1.0.0.schema.json"
    }
  ],
  "properties": {
    "TestSeries": {
      "type": "array",
      "items": {
        "type": "object",
        "if": {
          "$ref": "#/$defs/VDA278.TestSeriesCondition"
        },
        "then": {
          "$ref": "#/$defs/VDA278.TestSeries"
        }
      },
      "contains": {
        "$ref": "#/$defs/VDA278.TestSeriesCondition"
      }
    }
  },
  "$defs": {
    "VDA278.TestSeriesCondition": {
      "type": "object",
      "properties": {
        "Specification": {
          "type": "object",
          "properties": {
            "Type": {
              "type": "string",
              "const": "VDA"
            },
            "Number": {
              "type": "string",
              "const": "278"
            },
            "IssueDate": {
              "type": "string",
              "const": "2016-05"
            }
          }
        }
      }
    },
    "VDA278.TestSeries": {
      "type": "object",
      "properties": {
        "TestType": {
          "type": "string",
          "const": "Thermodesorption"
        },
        "Executions": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/VDA278.TestExecution"
          }
        },
        "ConsolidatedCharacteristicValues": {
          "type": "array",
          "minItems": 1,
          "items": {
            "anyOf": [
              {
                "$ref": "#/$defs/VDA278.ConsolidatedCharacteristicValueVOC1TotalEmission"
              },
              {
                "$ref": "#/$defs/VDA278.ConsolidatedCharacteristicValueVOC2TotalEmission"
              },
              {
                "$ref": "#/$defs/VDA278.ConsolidatedCharacteristicValueFOGTotalEmission"
              }
            ]            
          }
        }
      }
    },
    "VDA278.TestExecution": {
      "type": "object",
      "properties": {
        "Specimen": {
          "$ref": "#/$defs/VDA278.Specimen"
        },
        "SingleResults": {
          "type": "array",
          "minItems": 1,
          "items": {
            "anyOf": [
              {
                "$ref": "#/$defs/VDA278.SingleResultVOC"
              },
              {
                "$ref": "#/$defs/VDA278.SingleResultFOG"
              }
            ]
          }
        }
      },
      "required": [
        "Specimen",
        "SingleResults"
      ]
    },
    "VDA278.Specimen": {
      "type": "object",
      "properties": {
        "ProcessHistory": {
          "type": "array",
          "minItems": 1,
          "maxItems": 1,
          "prefixItems": [
            {
              "$ref": "#/$defs/VDA278.ProcessStepPreconditioning"
            }
          ]
        }
      }
    },
    "VDA278.ProcessStepPreconditioning": {
      "type": "object",
      "properties": {
        "Specification": {
          "type": "object",
          "const": {
            "Type": "VDA",
            "Number": "278",
            "IssueDate": "2016-05"
          }
        },
        "ProcessData": {
          "$ref": "#/$defs/VDA278.ProcessDataPreconditioning"
        }
      },
      "required": [
        "Specification",
        "ProcessData"
      ]
    },
    "VDA278.ProcessDataPreconditioning": {
      "type": "object",
      "properties": {
        "Designation": {
          "const": "Preconditioning"
        },
        "Attributes": {
          "type": "array",
          "minItems": 6,
          "maxItems": 6,
          "prefixItems": [
            {
              "$ref": "#/$defs/VDA278.ProcessDataPreconditioningDuration"
            },
            {
              "$ref": "#/$defs/VDA278.ProcessDataPreconditioningEndDate"
            },
            {
              "$ref": "#/$defs/VDA278.ProcessDataPreconditioningTempMin"
            },
            {
              "$ref": "#/$defs/VDA278.ProcessDataPreconditioningTempMax"
            },
            {
              "$ref": "#/$defs/VDA278.ProcessDataPreconditioningHumidMin"
            },
            {
              "$ref": "#/$defs/VDA278.ProcessDataPreconditioningHumidMax"
            }
          ]
        }
      },
      "required": [
        "Designation",
        "Attributes"
      ]
    },
    "VDA278.ProcessDataPreconditioningDuration": {
      "type": "object",
      "properties": {
        "Property": {
          "const": "Duration"
        },
        "Unit": {
          "const": "d"
        },
        "Value": {
          "type": "number",
          "minimum": 0.0
        }
      }
    },
    "VDA278.ProcessDataPreconditioningEndDate": {
      "type": "object",
      "properties": {
        "Property": {
          "const": "End date"
        },
        "Value": {
          "$ref": "https://vda231-301.github.io/schemas/generic/VDA_231-301_generic_v1.0.0.schema.json#/$defs/Generic.Date"
        }
      }
    },
    "VDA278.ProcessDataPreconditioningTempMin": {
      "type": "object",
      "properties": {
        "Property": {
          "const": "Temperature minimum"
        },
        "Unit": {
          "const": "°C"
        },
        "Value": {
          "type": "number"
        }
      }
    },
    "VDA278.ProcessDataPreconditioningTempMax": {
      "type": "object",
      "properties": {
        "Property": {
          "const": "Temperature maximum"
        },
        "Unit": {
          "const": "°C"
        },
        "Value": {
          "type": "number"
        }
      }
    },
    "VDA278.ProcessDataPreconditioningHumidMin": {
      "type": "object",
      "properties": {
        "Property": {
          "const": "Relative humidity minimum"
        },
        "Unit": {
          "const": "%"
        },
        "Value": {
          "type": "number",
          "minimum": 0.0,
          "maximum": 100.0
        }
      }
    },
    "VDA278.ProcessDataPreconditioningHumidMax": {
      "type": "object",
      "properties": {
        "Property": {
          "const": "Relative humidity maximum"
        },
        "Unit": {
          "const": "%"
        },
        "Value": {
          "type": "number",
          "minimum": 0.0,
          "maximum": 100.0
        }
      }
    },
    "VDA278.SingleResultSuper": {
      "type": "object",
      "properties": {
        "_id": {
          "$ref": "https://vda231-301.github.io/schemas/generic/VDA_231-301_generic_v1.0.0.schema.json#/$defs/Generic.Identifier"
        },
        "_type": {
          "type": "string",
          "const": "SingleResultPoint"
        },
        "Property": {
          "type": "string"
        },
        "Value": {
          "$ref": "#/$defs/VDA278.SingleResultValue"
        }
      },
      "required": [
        "_id",
        "_type",
        "Property",
        "Value"
      ]
    },
    "VDA278.SingleResultVOC": {
      "type": "object",
      "allOf": [
        {
          "$ref": "#/$defs/VDA278.SingleResultSuper"
        }
      ],
      "properties": {
        "Property": {
          "const": "VOC"
        }
      }
    },
    "VDA278.SingleResultFOG": {
      "type": "object",
      "allOf": [
        {
          "$ref": "#/$defs/VDA278.SingleResultSuper"
        }
      ],
      "properties": {
        "Property": {
          "const": "FOG"
        }
      }
    },
    "VDA278.SingleResultValue": {
      "type": "object",
      "properties": {
        "Method": {
          "$ref": "https://vda231-301.github.io/schemas/generic/VDA_231-301_generic_v1.0.0.schema.json#/$defs/Generic.RestrictedString"
        },
        "File name": {
          "$ref": "https://vda231-301.github.io/schemas/generic/VDA_231-301_generic_v1.0.0.schema.json#/$defs/Generic.RestrictedString"
        },
        "Vial number": {
          "$ref": "https://vda231-301.github.io/schemas/generic/VDA_231-301_generic_v1.0.0.schema.json#/$defs/Generic.RestrictedString"
        },
        "Remark": {
          "type": "string"
        },
        "Sum of identified substances area": {
          "type": "number",
          "minimum": 0.0,
          "maximum": 100.0
        },
        "Sum of identified substances emission": {
          "type": "number",
          "minimum": 0.0
        },
        "Emissions": {
          "type": "array",
          "minItems": 0,
          "items": {
            "$ref": "#/$defs/VDA278.SingleResultEmissionItem"
          }
        },
        "Classifications": {
          "type": "array",
          "minItems": 0,
          "items": {
            "$ref": "#/$defs/VDA278.SingleResultValueClassificationItem"
          }
        }
      },
      "required": [
        "Method",
        "File name",
        "Vial number",
        "Sum of identified substances area",
        "Sum of identified substances emission",
        "Emissions"
      ]
    },
    "VDA278.SingleResultEmissionItem": {
      "type": "object",
      "properties": {
        "Retention time": {
          "type": "number",
          "minimum": 0.0
        },
        "Substance name": {
          "type": [
            "string",
            "null"
          ]
        },
        "Legacy substance": {
          "type": [
            "string",
            "null"
          ]
        },
        "Mass fragments": {
          "type": "array",
          "items": {
            "type": "integer",
            "minimum": 0
          }
        },
        "Main mass fragment": {
          "type": "integer",
          "minimum": 0
        },
        "Potential substance match": {
          "type": [
            "string",
            "null"
          ]
        },
        "CAS registry number": {
          "oneOf": [
            {
              "type": "string",
              "pattern": "^[0-9]{2,7}[-][0-9]{2}[-][0-9]{1}$"
            },
            {
              "type": "null"
            }
          ]
        },
        "Area": {
          "type": "number",
          "minimum": 0.0,
          "maximum": 100.0
        },
        "Emission": {
          "type": "number",
          "minimum": 0.0
        },
        "Remark": {
          "type": [
            "string",
            "null"
          ]
        }
      },
      "required": [
        "Retention time",
        "Substance name",
        "Legacy substance",
        "Mass fragments",
        "Main mass fragment",
        "Potential substance match",
        "CAS registry number",
        "Area",
        "Emission",
        "Remark"
      ]
    },
    "VDA278.SingleResultValueClassificationItem": {
      "type": "object",
      "properties": {
        "CAS registry number": {
          "type": "string",
          "pattern": "^[0-9]{2,7}[-][0-9]{2}[-][0-9]{1}$"
        },
        "Regulations": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/VDA278.SingleResultValueClassificationBranchingRegulationItem"
          }
        }
      },
      "required": [
        "CAS registry number",
        "Regulations"
      ]
    },
    "VDA278.SingleResultValueClassificationBranchingRegulationItem": {
      "type": "object",
      "if": {
        "properties": {
          "Regulation": {
            "type": "string",
            "const": "GADSL"
          }
        }
      },
      "then": {
        "$ref": "#/$defs/VDA278.SingleResultValueClassificationGADSLRegulationItem"
      },
      "else": {
        "$ref": "#/$defs/VDA278.SingleResultValueClassificationRegulationItem"
      }
    },
    "VDA278.SingleResultValueClassificationRegulationItem": {
      "type": "object",
      "properties": {
        "Regulation": {
          "$ref": "https://vda231-301.github.io/schemas/generic/VDA_231-301_generic_v1.0.0.schema.json#/$defs/Generic.RestrictedString"
        },
        "Values": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://vda231-301.github.io/schemas/generic/VDA_231-301_generic_v1.0.0.schema.json#/$defs/Generic.RestrictedString"
          }
        }
      },
      "required": [
        "Regulation",
        "Values"
      ]
    },
    "VDA278.SingleResultValueClassificationGADSLRegulationItem": {
      "type": "object",
      "properties": {
        "Regulation": {
          "type": "string",
          "const": "GADSL"
        },
        "Values": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "oneOf": [
              {
                "const": "D"
              },
              {
                "const": "P"
              }
            ]
          }
        }
      },
      "required": [
        "Regulation",
        "Values"
      ]
    },
    "VDA278.ConsolidatedCharacteristicValueVOC1TotalEmission": {
      "type": "object",
      "properties": {
        "Property": {
          "type": "string",
          "const": "VOC total emission value"
        },
        "Unit": {
          "type": "string",
          "const": "µg/g"
        },
        "Value": {
          "type": "number",
          "minimum": 0.0
        },
        "Aggregation": {
          "type": "string",
          "const": "None"
        },
        "CriterionType": {
          "type": "string",
          "const": "Variable/Quantitative"
        }
      },
      "required": [
        "Property",
        "Unit",
        "Value",
        "Aggregation",
        "CriterionType"
      ]
    },
    "VDA278.ConsolidatedCharacteristicValueVOC2TotalEmission": {
      "type": "object",
      "properties": {
        "Property": {
          "type": "string",
          "const": "VOC second total emission value"
        },
        "Unit": {
          "type": "string",
          "const": "µg/g"
        },
        "Value": {
          "type": "number",
          "minimum": 0.0
        },
        "Aggregation": {
          "type": "string",
          "const": "None"
        },
        "CriterionType": {
          "type": "string",
          "const": "Variable/Quantitative"
        }
      },
      "required": [
        "Property",
        "Unit",
        "Value",
        "Aggregation",
        "CriterionType"
      ]
    },
    "VDA278.ConsolidatedCharacteristicValueFOGTotalEmission": {
      "type": "object",
      "properties": {
        "Property": {
          "type": "string",
          "const": "FOG total emission value"
        },
        "Unit": {
          "type": "string",
          "const": "µg/g"
        },
        "Value": {
          "type": "number",
          "minimum": 0.0
        },
        "Aggregation": {
          "type": "string",
          "const": "None"
        },
        "CriterionType": {
          "type": "string",
          "const": "Variable/Quantitative"
        }
      },
      "required": [
        "Property",
        "Unit",
        "Value",
        "Aggregation",
        "CriterionType"
      ]
    }
  }
}
